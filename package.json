{
  "name": "hashi-wallet",
  "version": "0.0.1",
  "description": "",
  "author": "ehanoc@protonmail.com / bruno.martins@algorand.foundation",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prestart:prod": "rimraf dist && npm run build",
    "build": "nest build -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "prettier": "prettier --write '{src,test}/**/*.ts' --no-semi --use-tabs true --tab-width 4 --print-width 160",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "test:cov": "jest src --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "vault:init": "npx tsx vault/init.ts",
    "vault:transit": "npx tsx vault/transit.ts",
    "vault:approle": "npx tsx vault/approle.ts",
    "vault:roles": "npx tsx vault/roles.ts"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "9.2.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/microservices": "9.2.0",
    "@nestjs/platform-express": "9.2.0",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@types/blake2b": "^2.1.0",
    "algo-msgpack-with-bigint": "^2.1.1",
    "algosdk": "^2.2.0",
    "axios": "^1.3.3",
    "base32": "^0.0.7",
    "bignumber.js": "^9.1.0",
    "bip39": "^3.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "hi-base32": "^0.5.1",
    "ioredis": "^5.2.4",
    "js-sha512": "^0.8.0",
    "json-bigint": "^1.0.0",
    "pg": "^8.8.0",
    "pq": "0.0.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "swagger-ui-express": "4.6.0",
    "tslib": "2.4.1",
    "tweetnacl": "^1.0.3",
    "typeorm": "0.3.10",
    "typescript": "4.8.4",
    "vlq": "^2.0.4"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "29.3.0",
    "jest-create-mock-instance": "^2.0.0",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "ts-lint": "4.5.1",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0"
  },
  "resolutions": {
    "tslib": "1.11.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../test/coverage",
    "collectCoverageFrom": [
      "**/!(*.module|*.interface|main|repl|exception.filter|logging.interceptor).{ts,js}"
    ],
    "coveragePathIgnorePatterns": [
      "/src/migration/"
    ],
    "testEnvironment": "node"
  }
}
